{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "bun run build.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "bun --hot src/index.ts",
    "format": "prettier --check . --cache --cache-location ./.cache/prettier",
    "format:fix": "prettier --check . --write --cache --cache-location ./.cache/prettier",
    "lint": "eslint . --cache --cache-location ./.cache/eslint",
    "lint:fix": "eslint . --cache --cache-location ./.cache/eslint --fix",
    "start": "NODE_ENV=production bun src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.2",
    "@tanstack/react-query": "^5.76.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.43.1",
    "elysia": "^1.3.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "latest",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "better-sqlite3": "^11.10.0",
    "bun-plugin-tailwind": "^0.0.14",
    "drizzle-kit": "^0.31.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript-eslint": "^8.32.1"
  }
}
